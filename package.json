{
  "name": "starknet-telegram-bot",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "server": "nodemon src/index.js",
    "client": "cd frontend && yarn start",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "install-all": "yarn install && cd frontend && yarn install",
    "clean-all": "rm -rf node_modules yarn.lock && cd frontend && rm -rf node_modules yarn.lock"
  },
  "dependencies": {
    "@libsql/client": "^0.14.0",
    "@web3modal/standalone": "^2.4.1",
    "@starknet-react/core": "^1.0.4",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "node-telegram-bot-api": "^0.61.0",
    "starknet": "^6.11.0",
    "tough-cookie": "^4.1.3",
    "request": "^2.88.2",
    "get-starknet": "^3.0.1",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
